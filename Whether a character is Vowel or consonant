#include <iostream>
using namespace std;

int main()
{
    char x;
    cout<<"Enter x : ";
    cin >> x;
    if(x =='a' || x =='e' || x =='i' || x =='o' || x =='u' || x =='A' || x =='E' || x =='I' || x =='O' || x =='U')
        cout << "x is a vowel";
    else 
        cout << "x is a consonant";
        
    return 0;
}

/*
OUTPUT
Enter x : E
x is a vowel
*/
