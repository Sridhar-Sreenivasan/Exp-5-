    cout<<"Enter b : ";
    cin >> b;
    cout<<"Enter c : ";
    cin >> c;
    
    if(a>b)
        if (a>c)
            cout << "a is largest no";
        else
            cout << "c is largest no";
    else 
        cout << "b is lasrgest no";
}
